<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE DomainModel>
<Domain Name="TrainingTesting" ShortName="TT">
  <Model Name="SalesOrder" Description="销售订单" ModelType="Document" Mapped="${Company}_TT_ORDR">
