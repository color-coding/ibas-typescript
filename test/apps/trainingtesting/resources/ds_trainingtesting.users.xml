  </Model>
  <BusinessObject MappedModel="SalesOrder" ShortName="${Company}_TT_SALESORDER">
    <RelatedBO Relation="OneToMany" MappedModel="SalesOrderItem">
      <RelatedBO Relation="OneToMany" MappedModel="BatchItem"/>
    </RelatedBO>
  </BusinessObject>
