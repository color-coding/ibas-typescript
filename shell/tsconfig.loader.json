{
  "compilerOptions": {
    "noImplicitAny": false,
    "removeComments": false,
    "target": "es6",
    "preserveConstEnums": true,
    "outFile": "./loader.js",
    "declaration": false,
    "sourceMap": false,
    "plugins": [
      {
        "transform": "ts-transform-inline-environment-variables",
        "vars": [
          "TS_BUILD_TIME"
        ]
      }
    ]
  },
  "include": [
    "../ibas/3rdparty/require.d.ts",
    "../ibas/index.d.ts"
  ],
  "files": [
    "./loader.ts"
  ]
}